{
	"name": "n8n-nodes-bandi-youtube-transcript",
	"version": "0.4.4",
	"description": "유튜브 자막 + 기타 유튜브 정보를 가져오는 n8n 커뮤니티 노드 패키지",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"youtube",
		"transcript",
		"youtube-transcript",
		"youtube-info"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "sweetburble",
		"email": "sweetburble2716@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/sweetburble/n8n-nodes-youtube-transcript.git"
	},
	"main": "index.js",
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint --no-error-on-unmatched-pattern nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"eslint": "9"
			},
			"ignoreMissing": [
				"eslint"
			]
		}
	},
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [],
		"nodes": [
			"dist/nodes/YoutubeTranscriptNode/YoutubeTranscriptNode.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"@typescript-eslint/utils": "^8.32.1",
		"eslint": "^9.27.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-core": "^1.14.1",
		"n8n-workflow": "^1.82.0",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.32.1"
	},
	"dependencies": {
		"youtubei": "^1.6.7"
	}
}
